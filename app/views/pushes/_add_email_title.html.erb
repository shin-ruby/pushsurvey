<fieldset>
  <legend><h4>Push Name:<%= @push.name %></h4></legend>
  <p>
    <label for="dummy1">Add Subject</label><br/>
    <%= f.text_field "subject" %><br/>
    <label for="dummy1">Add Email Signature</label><br/>
    <%= f.text_area "signature" %><br/>
    <label for="dummy1">From Email</label><br/>
    <%= f.text_field "from_email" %><br/>
    <label for="dummy1">Reply-To Email</label><br/>
    <%= f.text_field "reply_to_email" %>
  </p>
  <hr/>
  <p>
    <input type="reset" value="Cancel">
    <input type="submit" value="Continue">
  </p>
</fieldset>

<% content_for :note_sidebar do %>
    <fieldset>
      <legend><h4>Note</h4></legend>
      <ul id="content-pushC">
        <p><%= link_to image_tag("help/pushhelpd.png", :class=>"pushhelpd"), root_url %> </p>
      </ul>
    </fieldset>
<% end %>