    <div class="span-3">
        <div class="button">
            <%= link_to "Create Push", new_push_path(:type=>"new") %>
        </div>
    </div>
	<hr class="space" />
	<h2 style="color:#999">Push List</h2>
    <hr />
    <div class="span-24">
	    <p>Push List Note Address Book Below is a summary of the lists that are currently in your account. To view the details or modify an existing list, just click the name. To add a new list, simply click "Add New List".</p>
    </div>
    <hr />
	<div id="container">		
		<div id="demo">
            <table cellpadding="0" cellspacing="0" border="0" class="display" id="datagrid">
                <thead>
                    <tr>
                        <th>Push Title</th>
                        <th>Date Push</th>
                        <th>Design</th>
                        <th>Feedback</th>
                        <th>Status</th>
                        <th>Folder</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                    <% @pushes.each do |push| %>
                    <tr class="gradeX">
                        <td><%= link_to push.name, push %></td>
                        <td><%= push.date_push_presenter %></td>
                        <td><%= link_to image_tag("design.png"), push.design %></td>
                        <td class="center"><%= push.feedback_presenter %></td>
                        <td class="center"><%= push.status %></td>
                        <td class="center"><%= push.folder.html.html_safe %></td>

                          <td class="center"><% if push.date_push %><%= link_to "Export", export_push_path(push) %>|<%end %>

                        <%= link_to 'Delete', push, :confirm => 'Are you sure?', :method => :delete %></td>
                    </tr>
                    <% end %>
                </tbody>
                <tfoot>
                    <tr>
                        <th>Push Title</th>
                        <th>Date Push</th>
                        <th>Design</th>
                        <th>Feedback</th>
                        <th>Status</th>
                        <th>Folder</th>
                        <th>Action</th>
                    </tr>
                </tfoot>
            </table>
        </div>
        <div class="spacer"></div>		
      </div>


